        <SolidColorBrush x:Key="Button.Static.Border" Color="#FF282828"/>
        <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FF0080FF"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF0080FF"/>
        <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FF0059B2"/>
        <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF0059B2"/>
        <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
        <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
        <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
        <Style x:Key="ButtonStyle1" TargetType="{x:Type Button}">
